<h1>Create New User Form</h1>
<%=form_for @user do |f|%>
    <%=f.label :username%>
    <%=f.text_field :username%><br>
    <%=f.label :password%>
    <%=f.password_field :password%><br>
    <%=f.label :password_confirmation%>
    <%=f.password_field :password_confirmation%>
    <%=f.submit%>
<%end%>